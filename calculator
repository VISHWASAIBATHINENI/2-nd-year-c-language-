#include<stdio.h>
void main()
{
    int a,b ;
    printf("Enter two nums ");
    scanf("%d%d",&a,&b);
    char op;
    printf("\n Enter symbol");
    scanf("%c",&op);
    switch(op)
    {
        case '+': printf("The addition is %d",a+b);
                break;
        case '-': printf("The subtraction is %d",a-b);
                break;
        case '/': printf("The Division is %d",a/b);
                break;
        case '*': printf("The Multiplication is %d",a*b);
                break;
        default:
            printf("Wrong choice");

    }
}
